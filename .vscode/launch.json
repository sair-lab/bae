    "version": "0.2.0",
    "configurations": [
        {
            "env": {
                "TORCH_SHOW_DISPATCH_TRACE": "1"
            },
            "name": "Python: Debug Tests",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "purpose": ["debug-test"],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "env": {
                // "TORCH_SHOW_DISPATCH_TRACE": "1"
            },
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
        }
    ]
}